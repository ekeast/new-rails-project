<h1>Register a New Site</h1>

<div class="row">
  <div class="col-md-8">
    <%= form_for @registration, as: :registration, url: registered_applications_path, method: :post do |f| %>
    <div class="form-group">
      <%= f.label :name %>
      <%= f.text_field :name %>
    </div>
    <div class="form-group">
      <%= f.label :url %>
      <%= f.text_field :url %>
    </div>
    <div class="form-group">
      <%= f.submit "Save" %>
    </div>
    <% end %>
  </div>
</div>
